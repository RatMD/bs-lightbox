    "bugs": {
        "url": "https://github.com/RatMD/bs-lightbox",
        "email": "info@rat.md"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/RatMD/bs-lightbox.git"
    },
    "funding": [
        {
            "type": "buymeacoffee",
            "url": "https://buymeacoff.ee/ratmd"
        }
